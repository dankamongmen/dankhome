# General
Host *
	User dank
	Compression yes
	SendEnv LANG
	TCPKeepAlive yes
	#VerifyHostKeyDNS yes
	ServerAliveInterval 60
	ExitOnForwardFailure yes
	# ask is the default
	StrictHostKeyChecking yes
	ControlPath ~/tmp/ssh/%r-%l-%h-%p.ctl
